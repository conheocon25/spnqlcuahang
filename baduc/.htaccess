#---------------------------------------------------------------------------------------------------------------------------
# EXPORT
#---------------------------------------------------------------------------------------------------------------------------
RewriteRule ^export$ 															?cmd=Export [L]
RewriteRule ^export/big/([0-9]+)$ 												?cmd=ExportBig&IdDomain=$1 [L]
