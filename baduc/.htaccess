Options +FollowSymlinks
RewriteEngine on

## EXPIRES CACHING ##
<IfModule mod_expires.c>
ExpiresActive On
ExpiresByType image/jpg "access 1 year"
ExpiresByType image/jpeg "access 1 year"
ExpiresByType image/gif "access 1 year"
ExpiresByType image/png "access 1 year"
ExpiresByType text/css "access 1 month"
ExpiresByType text/html "access 1 month"
ExpiresByType application/pdf "access 1 month"
ExpiresByType text/x-javascript "access 1 month"
ExpiresByType application/x-shockwave-flash "access 1 month"
ExpiresByType image/x-icon "access 1 year"
ExpiresDefault "access 1 month"
</IfModule>
## EXPIRES CACHING ##

#---------------------------------------------------------------------------------------------------------------------------
# SYSTEM
#---------------------------------------------------------------------------------------------------------------------------
RewriteRule ^signin/load$ 			?cmd=SigninLoad [L]
RewriteRule ^signin/exe$ 			?cmd=SigninExe [L]
RewriteRule ^signout/exe$ 			?cmd=SignoutExe [L]

RewriteRule ^object/load/([A-Za-z0-9]+)/([0-9]+)$ 			?cmd=ObjectLoad&ObjectName=$1&Id=$2 [L]
RewriteRule ^object/del/([A-Za-z0-9]+)/([0-9]+)$ 			?cmd=ObjectDel&ObjectName=$1&Id=$2 [L]
RewriteRule ^object/ins/([A-Za-z0-9]+)$ 					?cmd=ObjectIns&ObjectName=$1 [L]
RewriteRule ^object/upd/([A-Za-z0-9]+)$ 					?cmd=ObjectUpd&ObjectName=$1 [L]

RewriteRule ^s2a/get/one/([A-Za-z0-9]+)/([0-9]+)$ 					?cmd=S2AGet&Type=one&ObjectName=$1&ObjectId=$2 [L]
RewriteRule ^s2a/get/all/([A-Za-z0-9]+)$ 							?cmd=S2AGet&Type=all&ObjectName=$1 [L]
RewriteRule ^s2a/get/first/([A-Za-z0-9]+)$ 							?cmd=S2AGet&Type=first&ObjectName=$1 [L]
RewriteRule ^s2a/get/last/([A-Za-z0-9]+)$ 							?cmd=S2AGet&Type=last&ObjectName=$1 [L]
RewriteRule ^s2a/get/size/([A-Za-z0-9]+)$ 							?cmd=S2AGet&Type=size&ObjectName=$1 [L]
RewriteRule ^s2a/get/page/([A-Za-z0-9]+)/([0-9]+)/([0-9]+)$ 		?cmd=S2AGet&Type=page&ObjectName=$1&Page=$2&RPP=$3 [L]
RewriteRule ^s2a/get/range/([A-Za-z0-9]+)/([0-9]+)/([0-9]+)$ 		?cmd=S2AGet&Type=range&ObjectName=$1&Range1=$2&Range2=$3 [L]
RewriteRule ^s2a/get/byid/([A-Za-z0-9]+)/([A-Za-z0-9]+)/([0-9]+)$ 	?cmd=S2AGet&Type=byid&ObjectName=$1&ObjectParentName=$2&ObjectParentId=$3 [L]

