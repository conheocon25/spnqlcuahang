RewriteRule ^report/customer/([0-9]+)/([0-9]+)/([0-9]+)/print$	?cmd=ReportCustomerTrackingDetailPrint&IdTrack=$1&IdCustomer=$2&IdCT=$3 [L]

RewriteRule ^report/collect/([0-9]+)$ 					?cmd=ReportCollectGeneral&IdTrack=$1 [L]
